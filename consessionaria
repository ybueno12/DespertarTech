void main() {

  int salarioBase = 3000;
  int comissaoBasica = 1000;
  int comissaoLuxo = 2000;
  int descontoFalta = 136;
  int vendasBasicas = 8;
  int vendasLuxo = 2;
  int faltas = 3;

  int salarioTotal = salarioBase + (vendasBasicas * comissaoBasica) + (vendasLuxo * comissaoLuxo) - (faltas * descontoFalta);

  print("O salário total do funcionário é: R$salarioTotal");

}
